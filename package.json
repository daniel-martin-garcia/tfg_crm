{
  "name": "CRM_DIT",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "heroku local:start",
    "start_old": "node ./bin/www",
    "supervisor": "./node_modules/.bin/supervisor ./bin/www",
    "migrate_local": ": ${DATABASE_URL:=sqlite://$(pwd)/crm.sqlite}; ./node_modules/.bin/sequelize db:migrate --url $DATABASE_URL",
    "seed_local": ": ${DATABASE_URL:=sqlite://$(pwd)/crm.sqlite}; ./node_modules/.bin/sequelize db:seed:all --url $DATABASE_URL",
    "open_heroku": "heroku open --app dcrmt",
    "migrate_heroku": "heroku run --app dcrmt ./node_modules/.bin/sequelize db:migrate --url \\$DATABASE_URL",
    "seed_heroku": "heroku run --app dcrmt ./node_modules/.bin/sequelize db:seed:all --url \\$DATABASE_URL",
    "reset_heroku": "heroku pg:reset DATABASE --app dcrmt",
    "mon_heroku": "heroku --app dcrmt maintenance:on",
    "moff_heroku": "heroku --app dcrmt maintenance:off"
  },
  "engines": {
    "node": "8.9.x",
    "npm": "4.6.x"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cloudinary": "^1.7.0",
    "connect-session-sequelize": "^5.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "express-flash": "0.0.2",
    "express-mailer": "^0.3.1",
    "express-partials": "^0.3.0",
    "express-session": "^1.14.2",
    "method-override": "^2.3.7",
    "micro": "^9.3.2",
    "moment": "^2.17.1",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "open": "0.0.5",
    "open-new-tab": "0.0.1",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.27.0",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "~2.3.0",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "sqlite3": "^3.1.8",
    "string-natural-compare": "^2.0.2",
    "supervisor": "^0.12.0"
  }
}
